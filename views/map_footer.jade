script(type="text/javascript", src="//code.jquery.com/jquery-2.1.1.min.js").
script(type="text/javascript", src="//cdnjs.cloudflare.com/ajax/libs/lodash.js/2.4.1/lodash.min.js").
script(type="text/javascript", src="http://maps.google.com/maps/api/js?key=AIzaSyDTnzQ8XNGVvpSmZ2Ds7DVZ4eVzWizBKoE").
script(type="text/javascript", src="javascripts/dist/app.js").
script.
  document.onreadystatechange = function(){
   if (document.readyState == "complete") {
      google.maps.event.addDomListener(window, 'load', function(){
        window.AppMap = new MunchiesMapView({
          centerPinPath: 'http://chart.apis.google.com/chart?chst=d_map_pin_letter&chld=%E2%80%A2|00D900'
        });
        window.AppList = new MunchiesListView();
      });
      document.body.classList.remove('hide');
    }
  };
